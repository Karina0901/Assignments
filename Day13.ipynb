    "import spacy\n",
    "\n",
    "def pos_tagging(text):\n",
    "    nlp = spacy.load(\"en_core_web_sm\")\n",
    "    doc = nlp(text)\n",
    "    for token in doc:\n",
    "        print(f\"{token.text}: {token.pos_}\")\n",
    "\n",
    "# Test\n",
    "text = \"NLP is amazing and fun to learn.\"\n",
    "pos_tagging(text)"
   ]
